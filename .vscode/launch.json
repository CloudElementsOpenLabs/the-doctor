    "version": "0.2.0",
    "configurations": [
        //for "program" don't use doctor.js as entry point, go directly to command file to prevent child process error
        {
            "type": "node",
            "request": "launch",
            "name": "download execution",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/src/cli/doctor-download.js",
            "protocol": "inspector",
            "args": [
                "execution",
                "production",
                "-f",
                "~/Desktop/execution.json",
                "-n",
                "1515022617"
            ],
            // "skipFiles": [
            //     "${workspaceFolder}/node_modules/**/*.js",
            //     "${workspaceFolder}/lib/**/*.js",
            //     "<node_internals>/**/*.js"
            // ],
            "console": "integratedTerminal",
        },
        {
            "type": "node",
            "request": "launch",
            "name": "download formulas",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/src/cli/doctor-download.js",
            "protocol": "inspector",
            "args": [
                "formulas",
                "production",
                "-f",
                "~/Desktop/formulas.json",
                "-n",
                "formulaNoSub"
            ],
            // "skipFiles": [
            //     "${workspaceFolder}/node_modules/**/*.js",
            //     "${workspaceFolder}/lib/**/*.js",
            //     "<node_internals>/**/*.js"
            // ],
            "console": "integratedTerminal",
        },
